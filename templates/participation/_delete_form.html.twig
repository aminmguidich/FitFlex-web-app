<form method="POST" action="{{ path('app_participation_delete', {'idpart': participation.idpart}) }}" onsubmit="return confirm('Are you sure you want to delete this item?');">
    <input type="hidden" name="_method" value="DELETE">
    <input type="hidden" name="_token" value="{{ csrf_token('delete' ~ participation.idpart) }}">
    <button type="submit">Delete</button>
</form>
