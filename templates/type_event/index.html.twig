{% extends 'base_front_ranim.html.twig' %}

{% block title %}Events index{% endblock %}
    {% block body %}

    <h1>TypeEvent index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Type</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for type_event in type_events %}
            <tr>
                <td>{{ type_event.id }}</td>
                <td>{{ type_event.type }}</td>
                <td>
                    <a href="{{ path('app_type_event_show', {'id': type_event.id}) }}">show</a>
                    {# <a href="{{ path('app_type_event_edit', {'id': type_event.id}) }}">edit</a> #}
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_type_event_new') }}">Create new</a>
    {% endblock %}

