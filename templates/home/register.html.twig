<!DOCTYPE html>
<html lang="en">

  <head>

    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">
    <meta name="description" content="">
    <meta name="author" content="">
    <link href="https://fonts.googleapis.com/css?family=Poppins:100,100i,200,200i,300,300i,400,400i,500,500i,600,600i,700,700i,800,800i,900,900i&display=swap" rel="stylesheet">

    <title>{% block tittre %}SALLE DE SPORT {% endblock %}</title>
<!--

TemplateMo 548 Training Studio

https://templatemo.com/tm-548-training-studio

-->
    <!-- Additional CSS Files -->
    {% block stylesheets %}
    <link rel="stylesheet" type="text/css" href="{{asset('FrontOffice/css/bootstrap.min.css')}}">

    <link rel="stylesheet" type="text/css" href="{{asset('FontOffice/css/font-awesome.css')}}">

    <link rel="stylesheet" href="{{asset('FrontOffice/css/templatemo-training-studio.css')}}">

    {% endblock %}

    </head>
    
    <body>
    
    <!-- ***** Preloader Start ***** -->
    <div id="js-preloader" class="js-preloader">
      <div class="preloader-inner">
        <span class="dot"></span>
        <div class="dots">
          <span></span>
          <span></span>
          <span></span>
        </div>
      </div>
    </div>
    <!-- ***** Preloader End ***** -->
    
    
    <!-- ***** Header Area Start ***** -->
    <header class="header-area header-sticky">
        <div class="container">
            <div class="row">
                <div class="col-12">
                    <nav class="main-nav">
                        <!-- ***** Logo Start ***** -->
                        <a href="{{ path('app_home') }}" class="logo">FETFLIX<em> GYM</em></a>
                        <!-- ***** Logo End ***** -->
                        <!-- ***** Menu Start ***** -->
                        <ul class="nav">
                            <li class="scroll-to-section"><a href="/#top" class="active">Home</a></li>
                             <li class="scroll-to-section"><a href="/#features">Abonnement</a></li> 

                            <li class="scroll-to-section"><a href="/#our-classes">Classes</a></li>
                            <li class="scroll-to-section"><a href="/#contact-us">Contact</a></li> 
                            <li class="main-button"><a href="/login">Login</a></li>
                        </ul>        
                        <a class='menu-trigger'>
                            <span>Menu</span>
                        </a>
                        <!-- ***** Menu End ***** -->
                    </nav>
                </div>
            </div>
        </div>
    </header>
    <!-- ***** Header Area End ***** -->

    <!-- ***** Main Banner Area Start ***** -->
    
    <div class="main-banner" id="top">
        <video autoplay muted loop id="bg-video">{ % block video %}
            <source src="{{asset('FrontOffice/images/gym-video.mp4')}}" type="video/mp4" />
            
        </video>

        <div class="video-overlay header-text">
            <div class="caption" style="z-index: 100%;">
            <div class="container">
                <div class="row justify-content-center">
                        <div class="col-lg-7">
                            <div class="card shadow-lg border-0 rounded-lg mt-5">
                                <div class="card-header">
                                    <h3 class="text-center font-weight-light my-4">Create Account</h3>
                                </div>
                                <div class="card-body">
                                    <form action="{{ path('user_save') }}" method="POST">
                                        <!-- First Row -->
                                        <div class="row mb-3">
                                            <div class="col-md-6">
                                                <div class="form-floating mb-3 mb-md-0">
                                                    <input class="form-control" id="fname" name="fname" type="text" placeholder="Enter your first name" />
                                                   
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-floating">
                                                 
                                                    <input class="form-control" id="lname" name="lname" type="text" placeholder="Enter your last name" />
                                                   
                                                </div>
                                            </div>
                                        </div>
                                        <!-- Second Row -->
                                        <div class="form-floating mb-3">
                                            <input class="form-control" id="email" name="email" type="email" placeholder="name@example.com" />
                                           
                                        </div>
                                        <!-- Third Row -->
                                        <div class="row mb-3">
                                            <div class="col-md-6">
                                                <div class="form-floating mb-3 mb-md-0">
                                                    <input class="form-control" id="password" name="password" type="password" placeholder="Create a password" />
                                                    
                                                </div>
                                            </div>
                                            <div class="col-md-6">
                                                <div class="form-floating">
                                                    <input class="form-control" id="age" type="number" name="age" placeholder="Enter your age" />
                                                    
                                                </div>
                                            </div>
                                            
                                            
                                        </div>
                                        <div class="col-md-6">
                                                <div class="form-floating">
                                                    <input class="form-control" id="numtel" type="number" name="numtel" placeholder="Enter your number" />
                                                    
                                                </div>
                                        </div>
                                         <input type="radio" id="sex1" name="sex" value="Homme">
                                            <label for="sex1">Homme</label>
                                         <input type="radio" id="sex2" name="sex" value="Femme">
                                            <label for="sex2">Femme</label><br>  
                                        <!-- Submit Button -->
                                        <button type="submit" class="btn btn-primary">Register</button>
                                        
                                    </form>
                                </div>
                                <div class="card-footer text-center py-3">
                                    <div class="small">
                                        <a href="{{ path('app_login') }}">Have an account? Go to login</a>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>
            </div>
        </div>
        </div>
        
    </div>
    
    <!-- ***** Main Banner Area End ***** -->

    <!-- ***** Features Item Start ***** -->



    
    <!-- ***** Footer Start ***** -->
    <footer>
        <div class="container">
            <div class="row">
                <div class="col-lg-12">
                    <p>Copyright &copy; 2020 Training Studio
                    
                    - Designed by <a rel="nofollow" href="https://templatemo.com" class="tm-text-link" target="_parent">TemplateMo</a><br>

                Distributed by <a rel="nofollow" href="https://themewagon.com" class="tm-text-link" target="_blank">ThemeWagon</a>
                
                </p>
                    
                    <!-- You shall support us a little via PayPal to info@templatemo.com -->
                    
                </div>
            </div>
        </div>
    </footer>

    <!-- jQuery -->
    <script src="{{asset('FrontOffice/js/jquery-2.1.0.min.js')}}"></script>

    <!-- Bootstrap -->
    <script src="{{asset('FrontOffice/js/popper.js')}}"></script>
    <script src="{{asset('FrontOffice/js/bootstrap.min.js')}}"></script>

    <!-- Plugins -->
    {% block javascripts %}
    <script src="{{asset('FrontOffice/js/scrollreveal.min.js')}}"></script>
    <script src="{{asset('FrontOffice/js/waypoints.min.js')}}"></script>
    <script src="{{asset('FrontOffice/js/jquery.counterup.min.js')}}"></script>
    <script src="{{asset('FrontOffice/js/imgfix.min.js')}}"></script> 
    <script src="{{asset('FrontOffice/js/mixitup.js')}}"></script> 
    <script src="{{asset('FrontOffice/js/accordions.js')}}"></script>
    
    <!-- Global Init -->
    <script src="{{asset('FrontOffice/js/custom.js')}}"></script>
 {% endblock %}
 
  </body>
</html>


