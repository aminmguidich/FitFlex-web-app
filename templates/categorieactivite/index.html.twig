{% extends 'base.html.twig' %}

{% block title %}Categorieactivite index{% endblock %}

{% block body %}
    <h1>Categorieactivite index</h1>

    <table class="table">
        <thead>
            <tr>
                <th>Id</th>
                <th>Categorie</th>
                <th>actions</th>
            </tr>
        </thead>
        <tbody>
        {% for categorieactivite in categorieactivites %}
            <tr>
                <td>{{ categorieactivite.id }}</td>
                <td>{{ categorieactivite.categorie }}</td>
                <td>
                    <a href="{{ path('app_categorieactivite_show', {'id': categorieactivite.id}) }}">show</a>
                    <a href="{{ path('app_categorieactivite_edit', {'id': categorieactivite.id}) }}">edit</a>
                </td>
            </tr>
        {% else %}
            <tr>
                <td colspan="3">no records found</td>
            </tr>
        {% endfor %}
        </tbody>
    </table>

    <a href="{{ path('app_categorieactivite_new') }}">Create new</a>
{% endblock %}
